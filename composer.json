    },
    "authors": [
        {
            "name": "Mike Thorn",
            "email": "mthorn@devlucid.com",
            "homepage": "http://devlucid.com",
            "role": "Developer"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "classmap": ["lib/php/lucid.php"]
    },
    "bin": [
        "bin/new-app",
        "bin/rebuild_models.php"
    ]

}
